@model WebProject.Models.TimeEntry

@{
    ViewData["Title"] = "Search"; 
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Task Details</h1> 

<div>
    <h4>Task Information</h4> 
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(p => p.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(p => p.Id)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(p => p.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(p => p.Date)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(p => p.Description)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(p => p.Description)
        </dd>
    </dl>
</div>

